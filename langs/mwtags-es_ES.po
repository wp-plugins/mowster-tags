#: index.php:53
#@ mwtags
msgid "Fetch tags"
msgstr "Buscar tags"

#: index.php:54
#@ mwtags
msgid "Fetching tags..."
msgstr "Buscando..."

#: ajax/ajax.php:33
#@ mwtags
msgid "Insufficient content text length."
msgstr "Contenido del texto insuficiente para sugerir tags."

#: ajax/ajax.php:70
#@ mwtags
msgid "Yahoo YQL did not find any tags for the content provided."
msgstr "El servidor de YQL Yahoo no encontró tags para el contenido suministrado."

#: index.php:186
#, php-format
#@ default
msgid "There is a new version of %1$s available. <a href=\"%2$s\" class=\"thickbox\" title=\"%3$s\">View version %4$s details</a> or <a href=\"%5$s\">update now</a>."
msgstr "Hay una versión más reciente de %1$s disponible. <a href=\"%2$s\" class=\"thickbox\" title=\"%3$s\">Ver los detalles de la versión %4$s</a> o <a href=\"%5$s\">actualizar ahora</a>."

#: ajax/ajax.php:62
#@ mwtags
msgid "Yahoo YQL server did not reply due one of the following reasons:"
msgstr "El servidor de YQL Yahoo no contestó por una de las siguientes razones:"

#: ajax/ajax.php:64
#@ mwtags
msgid "2. Your IP reached the 2000 limit requests per hour accepted, register at Yahoo for an API key."
msgstr "2. Su dirección IP alcanzó el limite de 2000 pedidos por hora, darse de alta en Yahoo para obtener una clave API."

#: ajax/ajax.php:63
#@ mwtags
msgid "1. Remote Yahoo service is down, try again within few seconds."
msgstr "1. El servidor de YQL Yahoo parece estar offline, inténtelo de nuevo en unos segundos."

#: ajax/ajax.php:44
#@ mwtags
msgid "Text length is over the limit handled by Yahoo. Only first 7000 chars were analyzed to find related tags."
msgstr "La longitud del texto es superior al límite permitido por Yahoo. Sólo se analizaron los primeros 7.000 caracteres para encontrar tags relacionadas."

